{
    "name": "filament/spatie-laravel-tags-plugin",
    "description": "Filament support for `spatie/laravel-tags`.",
    "license": "MIT",
    "homepage": "https://github.com/laravel-filament/spatie-laravel-tags-plugin",
    "support": {
        "issues": "https://github.com/laravel-filament/spatie-laravel-tags-plugin/issues",
        "source": "https://github.com/laravel-filament/spatie-laravel-tags-plugin"
    },
    "require": {
        "php": "^8.0",
        "filament/forms": "dev-develop",
        "spatie/laravel-tags": "^4.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.94"
    },
    "autoload": {
        "psr-4": {
            "Filament\\": "src"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpstan"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "local": {
            "type": "path",
            "url": "../forms"
        }
    }
}
